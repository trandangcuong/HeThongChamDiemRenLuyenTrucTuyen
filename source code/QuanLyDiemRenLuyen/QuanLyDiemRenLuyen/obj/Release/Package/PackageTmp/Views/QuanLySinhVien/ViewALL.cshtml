@model IEnumerable<QuanLyDiemRenLuyen.Models.SINHVIEN>

@{
    ViewBag.Title = "ViewALL";
    Layout = null;
}

<table class="table table-striped table-condensed" id="employeeTable1">
    <thead>
        <tr>
            <th>
                Tên sinh viên
            </th>
            <th>
                Ngày sinh
            </th>
            <th>
                Chức vụ
            </th>
            <th>
                Giới Tính
            </th>
            <th>
                Quê quán
            </th>
            <th>
                Lớp
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.sTenSV)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dNgaysinh)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sChucvu)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sGioitinh)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sQuequan)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FK_sMaLopID)
                </td>
                <td>
                    <a class="btn btn-primary btn-sm" onclick="Edit('@Url.Action("AddOrEdit", "QuanLySinhVien", new { id=@item.ID})')"><i class="fa fa-pencil fa-lg"></i></a>
                    <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete", "QuanLySinhVien", new { id=@item.ID})')"><i class="fa fa-trash fa-lg"></i></a>
                </td>
            </tr>
        }
    </tbody>
    </table>
  
