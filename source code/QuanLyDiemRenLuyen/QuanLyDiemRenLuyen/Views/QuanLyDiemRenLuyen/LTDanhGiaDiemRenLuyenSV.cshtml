        <h2 style="width:100%; text-align:center">Danh sách sinh viên đã đánh giá điểm rèn luyện</h2>
        </br>
    <ol class="rectangle-list" >
        @foreach (var item in Model)
        {
            <li><a   href = "@Url.Action("ChiTietSVDDG", "QuanLyDiemRenLuyen", new {ID = item.ID})"> @item.sTenSV</a></li>
          
        }
    </ol>
</div>