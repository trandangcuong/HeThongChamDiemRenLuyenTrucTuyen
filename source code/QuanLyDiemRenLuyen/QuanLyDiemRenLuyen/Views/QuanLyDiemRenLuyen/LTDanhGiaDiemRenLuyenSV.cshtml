@{
    ViewBag.Title = "Trang Chủ";
    Layout = "~/Views/Shared/_LayoutNguoiDung.cshtml";
}
<link href="~/Content/NguoiDung/danhsach.css" rel="stylesheet" />

@model List<QuanLyDiemRenLuyen.Models.sd_sinhviendanhgia_Result>
