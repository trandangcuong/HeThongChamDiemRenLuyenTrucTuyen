<!DOCTYPE html>

<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="GARO is a real-estate template">
    <meta name="author" content="Kimarotec">
    <meta name="keyword" content="html5, css, bootstrap, property, real-estate theme , bootstrap template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,700,800' rel='stylesheet' type='text/css'>
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/normalize.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/fontello.css">
    <link href="~/Content/NguoiDung/assets/fonts/icon-7-stroke/css/pe-icon-7-stroke.css" rel="stylesheet">
    <link href="~/Content/NguoiDung/assets/fonts/icon-7-stroke/css/helper.css" rel="stylesheet">
    <link href="~/Content/NguoiDung/assets/css/animate.css" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/bootstrap-select.min.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/icheck.min_all.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/price-range.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/owl.carousel.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/owl.theme.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/owl.transitions.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/style.css">
    <link rel="stylesheet" href="~/Content/NguoiDung/assets/css/responsive.css">
</head>


<body>
    @*<div id="preloader">
            <div id="status">&nbsp;</div>
        </div>*@
    <!-- Body content -->

    <div class="header-connect">
        <div class="container">
            <div class="row">
                <div class="col-md-5 col-sm-8  col-xs-12">
                    <div class="header-half header-call">
                        <p>
                            <span><i class="pe-7s-call"></i> +(0274) 382 2518</span>
                            <span><i class="pe-7s-mail"></i> tdmu@edu.com</span>
                        </p>
                    </div>
                </div>
                <div class="col-md-2 col-md-offset-5  col-sm-3 col-sm-offset-1  col-xs-12">
                    <div class="header-half header-social">
                        <ul class="list-inline">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-vine"></i></a></li>
                            <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                            <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--End top header -->

    <nav class="navbar navbar-default ">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header" style="height: 80px">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navigation">

                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html"><img src="~/Content/NguoiDung/assets/img/demo/download.png" width="422" height="119"></a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse yamm" id="navigation">
                <div class="button navbar-right">
                    <ul>
                        @if (Session["TaiKhoan"] != null)
                        {
                            <li><a style="color:black;"> @Session["TenNguoiDung"]</a></li>

                            <li class="m_2"><a href="@Url.Action("DangXuat", "QuanLyTaiKhoan")"> Đăng xuất</a></li>
                        }
                    </ul>
                </div>
                <ul class="main-nav nav navbar-nav navbar-right" style="height: 85px;">
                    <li class="dropdown ymm-sw " data-wow-delay="0.1s">
                        <a href="index.html" class="dropdown-toggle active" data-toggle="dropdown" data-hover="dropdown" data-delay="200">Trang Chủ</a>

                    </li>

                    <li class="wow fadeInDown" data-wow-delay="0.1s"><a>Thông Tin Sinh Viên</a></li>
                    <li class="dropdown ymm-sw " data-wow-delay="0.1s">
                        <a href="" data-toggle="dropdown" data-hover="dropdown" data-delay="200">Điểm Rèn Luyện</a>
                        <ul class="dropdown-menu navbar-nav">
                              @if (Session["TaiKhoan"] != null)
                              {
                                  if(int.Parse(Session["QuyenId"].ToString()) == 4)
                                  { 

                            <li>
                                <a href="@Url.Action("XemDiemRenLuyen","QuanLyDiemRenLuyen",new {MSV=Session["masv"].ToString()})">Xem Điểm Rèn Luyện</a>
                            </li>
                            <li>
                                <a href="@Url.Action("DanhGiaDiemRenLuyen","QuanLyDiemRenLuyen")">Sinh Viên Tự Đánh Giá Điểm Rèn Luyện</a>
                            </li>

                                  }

                                  else if (int.Parse(Session["QuyenId"].ToString()) == 3)
                                  {
                                    <li>
                                        <a href="@Url.Action("XemDiemRenLuyen","QuanLyDiemRenLuyen",new {MSV=Session["masv"].ToString()})">Xem Điểm Rèn Luyện</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("DanhSachXemDiem","QuanLyDiemRenLuyen")">Danh Sách Xem Điểm Rèn Luyện</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("DanhGiaDiemRenLuyen","QuanLyDiemRenLuyen")">Sinh Viên Tự Đánh Giá Điểm Rèn Luyện</a>
                                    </li>

                                    <li>
                                        <a  href="@Url.Action("LTDanhGiaDiemRenLuyenSV", "QuanLyDiemRenLuyen")">Cán Bộ Lớp Duyệt Điểm Rèn Luyện</a>
                                    </li>
                                  }
                                  else if (int.Parse(Session["QuyenId"].ToString()) == 2)

                                  {
                                    <li>
                                        <a href="@Url.Action("XemDiemRenLuyen","QuanLyDiemRenLuyen")">Xem Điểm Rèn Luyện</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("LTDanhGiaDiemRenLuyenGV","QuanLyDiemRenLuyen")">Giáo Viên Duyệt Điểm Rèn Luyện</a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("DanhsachChinhSuaDGGV","QuanLyDiemRenLuyen")">Chỉnh sửa điểm rèn luyện sinh viên. </a>
                                    </li>
                                  }

                              }
                        </ul>

                    </li>
                    <li class="wow fadeInDown" data-wow-delay="0.4s"><a>Tài Khoản</a></li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
    <!-- End of nav bar -->
    <div>
        @RenderBody()
    </div>
  
    <div class="footer-copy text-center" style="margin-top:300px;">
        <div class="container">
            <div class="row">
                <div class="pull-left">
                    <span> (C) <a href="http://tdmu.edu.vn"> 2019 Đại Học Thủ Dầu Một</a> </span>
                </div>
                <div class="bottom-menu pull-right">
                    <ul>
                        <li><a class="wow fadeInUp animated" href="#" data-wow-delay="0.2s">Trang Chủ</a></li>
                        <li><a class="wow fadeInUp animated" href="#" data-wow-delay="0.3s">Thông Tin Sinh Viên</a></li>
                        <li><a class="wow fadeInUp animated" href="#" data-wow-delay="0.4s">Điểm Rèn Luyện</a></li>
                        <li><a class="wow fadeInUp animated" href="#" data-wow-delay="0.6s">Tài Khoản</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/NguoiDung/assets/js/icheck.min.js"></script>
    <script src="~/Content/NguoiDung/assets/js/jquery-1.10.2.min.js"></script>


    <script src="~/Content/NguoiDung/assets/js/modernizr-2.6.2.min.js"></script>

    <script src="~/Content/NguoiDung/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/NguoiDung/assets/js/bootstrap-select.min.js"></script>
    <script src="~/Content/NguoiDung/assets/js/bootstrap-hover-dropdown.js"></script>

    <script src="~/Content/NguoiDung/assets/js/easypiechart.min.js"></script>
    <script src="~/Content/NguoiDung/assets/js/jquery.easypiechart.min.js"></script>

    <script src="~/Content/NguoiDung/assets/js/owl.carousel.min.js"></script>
    <script src="~/Content/NguoiDung/assets/js/wow.js"></script>

    
    <script src="~/Content/NguoiDung/assets/js/price-range.js"></script>

    <script src="~/Content/NguoiDung/assets/js/main.js"></script>

    
</body>
</html>
